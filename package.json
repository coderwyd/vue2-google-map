{
  "name": "vue3-google-map",
  "type": "module",
  "version": "0.21.0",
  "description": "A set of composable components for easy use of Google Maps in your Vue 3 projects.",
  "license": "MIT",
  "homepage": "https://vue3-google-map.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/inocan-group/vue3-google-maps.git"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./themes": {
      "types": "./dist/types/themes/index.d.ts",
      "import": "./dist/themes/index.mjs",
      "require": "./dist/themes/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "themes": [
        "dist/types/themes/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "dev": "vite",
    "lint": "npx eslint . --fix",
    "build": "vite build",
    "release": "standard-version"
  },
  "peerDependencies": {
    "vue": "^2.7.16"
  },
  "dependencies": {
    "@googlemaps/js-api-loader": "^1.16.8",
    "@googlemaps/markerclusterer": "^2.5.3",
    "fast-deep-equal": "^3.1.3"
  },
  "devDependencies": {
    "@coderwyd/eslint-config": "^2.6.4",
    "@types/google.maps": "^3.55.12",
    "@types/node": "^22.2.0",
    "@vitejs/plugin-vue2": "^2.3.1",
    "bumpp": "^9.5.1",
    "eslint": "^9.9.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.0",
    "vitepress": "^1.3.2",
    "vue": "^2.7.16",
    "vue-tsc": "^2.0.29"
  },
  "pnpm": {
    "allowedDeprecatedVersions": {
      "vue": "2.x"
    }
  }
}
